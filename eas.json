{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "env": {
        "EXPO_PUBLIC_SPOTIFY_CLIENT_ID": "PLACEHOLDER_FOR_SPOTIFY_CLIENT_ID",
        "EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY": "PLACEHOLDER_FOR_STRIPE_PUBLISHABLE_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}